{
  "expo": {
    "name": "Angaza Elimu",
    "slug": "angazaelimu",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.angazaelimu.angaza",
      "permissions": ["WIFI_STATE", "INTERNET", "WRITE_EXTERNAL_STORAGE","WRITE_SETTINGS", "READ_SETTINGS" ],
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
